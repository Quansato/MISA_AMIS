,iBAAA,GAAA,CAAA;AACA;;AAEA,aAAA,KAAA,WAAA,GAAA,CAAA;AACA,KAZA;AAcA;AACA,IAAA,OAfA,qBAeA;AACA,aAAA,IAAA,CAAA,GAAA,CACA,KAAA,SAAA,GAAA,KAAA,iBAAA,GAAA,CADA,EAEA,KAAA,UAFA,CAAA;AAIA,KApBA;AAsBA;AACA,IAAA,KAvBA,mBAuBA;AACA,UAAA,KAAA,GAAA,EAAA;;AAEA,WAAA,IAAA,CAAA,GAAA,KAAA,SAAA,EAAA,CAAA,IAAA,KAAA,OAAA,EAAA,CAAA,IAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,CADA;AAEA,UAAA,UAAA,EAAA,CAAA,KAAA,KAAA;AAFA,SAAA;AAIA;;AAEA,aAAA,KAAA;AACA,KAlCA;AAmCA;AACA,IAAA,aApCA,2BAoCA;AACA,aAAA,KAAA,WAAA,KAAA,CAAA;AACA,KAtCA;AAuCA;AACA,IAAA,YAxCA,0BAwCA;AACA,aAAA,KAAA,WAAA,KAAA,KAAA,UAAA;AACA;AA1CA,GAzBA;AAqEA,EAAA,OAAA,EAAA;AACA;AACA;AACA;AACA;AACA,IAAA,gBALA,8BAKA;AACA,WAAA,KAAA,CAAA,aAAA,EAAA,CAAA;AACA,KAPA;;AASA;AACA;AACA;AACA;AACA,IAAA,mBAbA,iCAaA;AACA,WAAA,KAAA,CAAA,aAAA,EAAA,KAAA,WAAA,GAAA,CAAA;AACA,KAfA;;AAiBA;AACA;AACA;AACA;AACA,IAAA,WArBA,uBAqBA,IArBA,EAqBA,IArBA,EAqBA;AACA,UAAA,IAAA,EAAA;AACA,aAAA,MAAA,CAAA,MAAA,CAAA,aAAA,EAAA,IAAA;AACA;;AACA,WAAA,KAAA,CAAA,aAAA,EAAA,IAAA,EAAA,IAAA;AACA,KA1BA;;AA4BA;AACA;AACA;AACA;AACA,IAAA,eAhCA,6BAgCA;AACA,WAAA,KAAA,CAAA,aAAA,EAAA,KAAA,WAAA,GAAA,CAAA;AACA,KAlCA;;AAoCA;AACA;AACA;AACA;AACA,IAAA,eAxCA,6BAwCA;AACA,WAAA,KAAA,CAAA,aAAA,EAAA,KAAA,UAAA;AACA,KA1CA;;AA4CA;AACA;AACA;AACA;AACA;AACA,IAAA,YAjDA,wBAiDA,IAjDA,EAiDA;AACA,aAAA,KAAA,WAAA,KAAA,IAAA;AACA;AAnDA;AArEA,CAAA","sourcesContent":["<template>\r\n    <div class=\"pagination items-center\">\r\n        <div class=\"total-record\">\r\n            Tổng số: {{total}} bản ghi\r\n        </div>\r\n        <div class=\"select flex items-center\">\r\n            <select name=\"\" id=\"\" @change=\"onClickPage(1,$event.target.value)\">\r\n                <option value=\"10\">10 bản ghi trên 1 trang</option>\r\n                <option value=\"20\">20 bản ghi trên 1 trang</option>\r\n                <option value=\"30\">30 bản ghi trên 1 trang</option>\r\n                <option value=\"40\">40 bản ghi trên 1 trang</option>\r\n                <option value=\"50\">50 bản ghi trên 1 trang</option>\r\n            </select>\r\n            <button class=\"btn-previous mr-4\"\r\n                    @click=\"onClickPreviousPage\"\r\n                    :disabled=\"isInFirstPage\">\r\n                Trước\r\n            </button>\r\n            <div class=\"page flex\">\r\n                <div v-for=\"page in pages\"\r\n                     :key=\"page.name\"\r\n                     @click=\"onClickPage(page.name)\"\r\n                     :class=\"{ page_active: isPageActive(page.name) }\"\r\n                     class=\"page-number\"\r\n                     :disabled=\"page.isDisabled\">\r\n                    {{ page.name }}\r\n                </div>\r\n\r\n                <!-- <div class=\"page-number page-active\">1</div>\r\n                <div class=\"page-number\">2</div> -->\r\n            </div>\r\n            <button class=\"btn-next ml-4\"\r\n                    @click=\"onClickNextPage\"\r\n                    :disabled=\"isInLastPage\">\r\n                Sau\r\n            </button>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"Pagination\",\r\n        props: {\r\n            maxVisibleButtons: {\r\n                type: Number,\r\n                required: false,\r\n                default: 3,\r\n            },\r\n            totalPages: {\r\n                type: Number,\r\n                required: true,\r\n            },\r\n            total: {\r\n                type: Number,\r\n                required: true,\r\n            },\r\n            perPage: {\r\n                type: Number,\r\n                required: true,\r\n            },\r\n            currentPage: {\r\n                type: Number,\r\n                required: true,\r\n            },\r\n        },\r\n        computed: {\r\n            //trang đầu\r\n            startPage() {\r\n                if (this.currentPage === 1) {\r\n                    return 1;\r\n                }\r\n\r\n                if (this.currentPage === this.totalPages) {\r\n                    return this.totalPages - this.maxVisibleButtons + 1;\r\n                }\r\n\r\n                return this.currentPage - 1;\r\n            },\r\n\r\n            //Lấy trang cuối\r\n            endPage() {\r\n                return Math.min(\r\n                    this.startPage + this.maxVisibleButtons - 1,\r\n                    this.totalPages\r\n                );\r\n            },\r\n\r\n            //chia tổng trang ra thành nhiều trang nhỏ\r\n            pages() {\r\n                const range = [];\r\n\r\n                for (let i = this.startPage; i <= this.endPage; i += 1) {\r\n                    range.push({\r\n                        name: i,\r\n                        isDisabled: i === this.currentPage,\r\n                    });\r\n                }\r\n\r\n                return range;\r\n            },\r\n            //đang ởtrang đầu tiên hay không\r\n            isInFirstPage() {\r\n                return this.currentPage === 1;\r\n            },\r\n            //đang ở trang cuối cùng hay không\r\n            isInLastPage() {\r\n                return this.currentPage === this.totalPages;\r\n            },\r\n        },\r\n        methods: {\r\n            /**\r\n             * Tới trang đầu tiên\r\n             * CreatedBy: ntquan(14/05/2021)\r\n             * */\r\n            onClickFirstPage() {\r\n                this.$emit(\"pagechanged\", 1);\r\n            },\r\n\r\n            /**\r\n            * Tới trang trước đó\r\n            * CreatedBy: ntquan(14/05/2021)\r\n            * */\r\n            onClickPreviousPage() {\r\n                this.$emit(\"pagechanged\", this.currentPage - 1);\r\n            },\r\n\r\n            /**\r\n            * Tới trang {page}\r\n            * CreatedBy: ntquan(11/05/2021)\r\n            * */\r\n            onClickPage(page, size) {\r\n                if (size) {\r\n                    this.$store.commit(\"setPageSize\", size)\r\n                }\r\n                this.$emit(\"pagechanged\", page, size);\r\n            },\r\n\r\n            /**\r\n            * Tới trang tiếp theo\r\n            * CreatedBy: ntquan(11/05/2021)\r\n            * */\r\n            onClickNextPage() {\r\n                this.$emit(\"pagechanged\", this.currentPage + 1);\r\n            },\r\n\r\n            /**\r\n            * Tới trang cuối\r\n            * CreatedBy: ntquan(11/05/2021)\r\n            * */\r\n            onClickLastPage() {\r\n                this.$emit(\"pagechanged\", this.totalPages);\r\n            },\r\n\r\n            /**\r\n             * xác định trang đang chọn\r\n             * @param page trang\r\n             * CreatedBy: ntquan(11/05/2021)\r\n             */\r\n            isPageActive(page) {\r\n                return this.currentPage === page;\r\n            },\r\n        },\r\n    };\r\n</script>\r\n\r\n<style>\r\n    .pagination {\r\n        position: absolute;\r\n        bottom: 10px;\r\n        z-index: 6;\r\n        background-color: #fff;\r\n        display: flex;\r\n        min-height: 47px;\r\n        padding-left: 16px;\r\n        padding-right: 30px;\r\n        width: 93%;\r\n        justify-content: space-between;\r\n    }\r\n\r\n        .pagination .select {\r\n            justify-content: space-between;\r\n        }\r\n\r\n            .pagination .select select {\r\n                margin: 0 16px;\r\n                width: 200px;\r\n                min-width: 200px;\r\n                min-height: 32px;\r\n                border: 1px solid #babec5;\r\n                border-radius: 2px;\r\n                background-color: #fff;\r\n                overflow: hidden;\r\n            }\r\n\r\n    .right-5 {\r\n        right: 5px;\r\n    }\r\n\r\n    .m-dropdown--menu {\r\n        position: absolute;\r\n        background-color: #fff;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components/pagination"}]}